Proceso Punto4
	
	Definir cuadrado, doble, cubo, triple, n Como Real;
	Escribir 'ingrese un numero:' ;
	Leer n;
	
	cuadrado <- n*n;
	cubo <- n*n*n;
	doble <- n*2;
	triple <- n*3;
	
	Escribir 'el cuadrado de ' n ' es ' cuadrado;
	Escribir 'el cubo de ' n ' es ' cubo;
	Escribir ' el doble de ' n ' es ' doble;
	Escribir 'el triple de' n ' es ' triple;
	
FinProceso
